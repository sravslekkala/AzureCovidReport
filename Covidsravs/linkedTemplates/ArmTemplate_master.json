{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Covidsravs"
		},
		"ls_blobstorage_covidsravs_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobstorage_covidsravs'"
		},
		"ls_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks'"
		},
		"ls_gen2_covidsravs_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_gen2_covidsravs'"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1203-234322-n5bk1wk2"
		},
		"ls_gen2_covidsravs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidsravsdl.dfs.core.windows.net/"
		},
		"ls_get_EDCD_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e5defa48-3dbe-4866-836a-894f1036b49b/resourceGroups/covidrg/providers/Microsoft.Storage/storageAccounts/covidsravs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Covidsravs_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blobstorage_covidsravs_connectionString": {
						"value": "[parameters('ls_blobstorage_covidsravs_connectionString')]"
					},
					"ls_databricks_accessToken": {
						"value": "[parameters('ls_databricks_accessToken')]"
					},
					"ls_gen2_covidsravs_accountKey": {
						"value": "[parameters('ls_gen2_covidsravs_accountKey')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_gen2_covidsravs_properties_typeProperties_url": {
						"value": "[parameters('ls_gen2_covidsravs_properties_typeProperties_url')]"
					},
					"ls_get_EDCD_data_properties_typeProperties_url": {
						"value": "[parameters('ls_get_EDCD_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Covidsravs_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Covidsravs_ArmTemplate_0')]"
			]
		}
	]
}